{
  "scripts": {
    "lint": "./node_modules/.bin/eslint src",
    "start": "./node_modules/.bin/parcel index.html --port 3000 --out-dir build-client-dev",
    "build": "rm -rf ./build-client && ./node_modules/.bin/parcel build index.html --out-dir build-client",
    "cleanup": "rm -rf node_modules && rm -rf .cache && rm -rf build-client",
    "release:clean": "rm -rf ../webapp/build-client",
    "release:copy": "cp -r ./build-client ../webapp/build-client",
    "release:sub": "sed -i -e 's/REDUX_INITIAL_STATE={}/REDUX_INITIAL_STATE={app:{backend:\"\\/\"}}/g' ../webapp/build-client/index.html && rm ../webapp/build-client/index.html-e",
    "release": "yarn release:clean && yarn build && yarn release:copy && yarn release:sub"
  },
  "dependencies": {
    "@marcopeg/utils": "1.12.0",
    "babel-polyfill": "^6.26.0",
    "features": "^0.1.0",
    "history": "^4.7.2",
    "moment-duration-format": "^2.2.2",
    "moment-timezone": "^0.5.23",
    "prop-type": "^0.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-container-dimensions": "^1.4.1",
    "react-device-detect": "^1.6.1",
    "react-dom": "^16.2.0",
    "react-fastclick": "^3.0.2",
    "react-icons": "^3.2.2",
    "react-loadable": "^5.5.0",
    "react-loader-spinner": "^2.2.0",
    "react-redux": "^5.1.0",
    "react-redux-feature": "^0.3.2",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-events-middleware": "^1.5.1",
    "redux-thunk": "^2.3.0",
    "slugify": "^1.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "^2.10.0",
    "parcel-bundler": "^1.10.3",
    "react-hot-loader": "^4.3.12"
  }
